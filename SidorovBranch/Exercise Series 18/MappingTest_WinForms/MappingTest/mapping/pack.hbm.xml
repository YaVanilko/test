<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly=" MappingTest"
                   namespace=" MappingTest.Entities">

  <class name="Pack" table="[Pack]" lazy="true">

    <id name="Id" column="ID" type ="int" >

      <generator class="identity" />
    </id>
    <property name="Barcode" column="barcode"  type ="string" />

    <property name="Weight" column="weight"  type ="string" />
    
    <property name="OrderId" column="Order_ID"  type ="int" />

  </class>
</hibernate-mapping>